---
import Layout from '../layouts/Layout.astro';

// SEO Constants
const TITLE = "Organic Coconut Sugar Manufacturer Indonesia | Bulk & OEM Supplier";
const DESCRIPTION = "Direct factory supplier of Organic Coconut Sugar (Granulated, Nectar, Block). HACCP, USDA, EU Organic certified. Low moisture (<2%), High solubility. Global shipping.";

// 1. PRODUCT SCHEMA (Rich Result)
const productSchema = {
  "@context": "https://schema.org/",
  "@type": "Product",
  "name": "Organic Coconut Sugar Bulk (Export Grade)",
  "image": "https://placehold.co/800x800/png?text=Organic+Coconut+Sugar+Bulk",
  "description": "Certified organic coconut sugar for industrial application. Low glycemic index, unrefined, and high nutrient profile. Available in 25kg bulk packaging.",
  "brand": { "@type": "Brand", "name": "Tropical Coco Indonesia" },
  "sku": "TCI-BULK-001",
  "offers": {
    "@type": "AggregateOffer",
    "priceCurrency": "USD",
    "lowPrice": "1600",
    "highPrice": "2400",
    "offerCount": "10",
    "availability": "https://schema.org/InStock"
  }
};

// 2. FAQ SCHEMA (Untuk Google 'People Also Ask')
const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "What is the Minimum Order Quantity (MOQ) for export?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "For Bulk packaging (25kg sacks), our MOQ is 1,000 kg (1 Ton) for LCL shipments. For Full Container Load (FCL), we recommend 20ft (approx 18 Tons) for cost efficiency."
      }
    },
    {
      "@type": "Question",
      "name": "Are your products certified organic?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, we hold valid certificates for USDA NOP, EU Organic, and JAS. We are also HACCP and Halal certified."
      }
    },
    {
      "@type": "Question",
      "name": "Can you do Private Label / OEM?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, we provide end-to-end OEM services. We can pack in standing pouches (250g, 500g, 1kg) or jars with your brand design."
      }
    }
  ]
};
---

<Layout title={TITLE} description={DESCRIPTION}>
  
  <fragment slot="head-schema">
    <script type="application/ld+json" set:html={JSON.stringify(productSchema)} />
    <script type="application/ld+json" set:html={JSON.stringify(faqSchema)} />
  </fragment>

  <section class="relative bg-white border-b border-gray-200">
      <div class="grid lg:grid-cols-2 min-h-[90vh]">
          
          <div class="flex flex-col justify-center px-6 lg:px-16 py-12 lg:py-0 order-2 lg:order-1 bg-white">
              <div class="inline-flex items-center gap-2 mb-6">
                  <span class="w-2 h-2 rounded-full bg-green-500 animate-pulse"></span>
                  <span class="text-xs font-bold tracking-widest text-gray-500 uppercase">Ready Stock for Export</span>
              </div>
              
              <h1 class="text-4xl lg:text-6xl font-extrabold text-coco-dark leading-[1.1] mb-6">
                  Consistent Quality.<br>
                  <span class="text-transparent bg-clip-text bg-gradient-to-r from-coco-gold to-yellow-600">Industrial Scale.</span>
              </h1>
              
              <p class="text-lg text-gray-600 mb-8 leading-relaxed max-w-lg">
                  We solve the supply chain instability of natural sweeteners. Get <strong>Standardized Organic Coconut Sugar</strong> directly from our factory in East Java.
              </p>

              <div class="flex flex-col sm:flex-row gap-4">
